name.replace(\">\", \"&gt;\");\n              verified_name = verified_name.replace(\"#\", \"\");\n              verified_name = verified_name.replace(\"$\", \"\");\n              verified_name = verified_name.replace(\"|\", \"\");\n              verified_name = verified_name.replace(\"^\", \"\");\n              app\n                .database()\n                .ref(\"users\")\n                .child(\"influencers\")\n                .child(res.user.uid)\n                .set({\n                  is_completed: false,\n                  full_name: verified_name,\n                });\n              history.push(\"/influencerdetails\"); //redirect to influencer detail page because the influencer have not provided his details\n            } else {\n              app\n                .database()\n                .ref(\"users\")\n                .child(\"influencers\")\n                .child(res.user.uid)\n                .child(\"is_completed\")\n                .get()\n                .then((result) => {\n                  if (result.val()) {\n                    history.push(\"/influencer\"); //redirect to dashboard\n                  } else {\n                    history.push(\"/influencerdetails\");\n                  }\n                });\n            }\n          });\n      })\n      .catch((error) => {\n        console.log(error.message);\n      });\n  };\n  const handleSignUp = useCallback(\n    async (event) => {\n      event.preventDefault();\n      const { email, password, username } = event.target.elements;\n      try {\n        await app\n          .auth()\n          .createUserWithEmailAndPassword(email.value, password.value); // create a new user with the provided details\n        firebase.auth().onAuthStateChanged((user) => {\n          if (user) {\n            let verified_name = username.value.replace(\".\", \"\");\n            verified_name = verified_name.replace(\"<\", \"&lt;\");\n            verified_name = verified_name.replace(\">\", \"&gt;\");\
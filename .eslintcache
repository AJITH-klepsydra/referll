}>\n            <a>Sign Up</a>\n          </button>\n        </div>\n\n        <p className=\"text_p\">\n          Already have an account?{\" \"}\n          <Link className=\"linked\" to=\"/login\">\n            Login\n          </Link>\n        </p>\n      </form>\n      <br />\n    </React.Fragment>\n  );\n};\n\nexport default withRouter(SignUp);\n","/home/klepsydra/Documents/Projects/Javascript/referl/src/components/profile/influencerdetails.js",[],"/home/klepsydra/Documents/Projects/Javascript/referl/src/components/pagenotfound/NoteFound.js",[],"/home/klepsydra/Documents/Projects/Javascript/referl/src/components/home/Landing.js",["82","83","84","85","86","87","88","89","90","91","92","93","94","95","96","97","98","99","100","101","102","103","104","105","106","107","108","109","110","111"],"import React from \"react\";\nimport {Link} from \"react-router-dom\";\nimport \"./Landing.css\";\nimport \"./Landingresponse.css\";\nimport hero from \"../../assets/hero-img.svg\";\nimport ban from \"../../assets/ban.png\";\nimport horn from \"../../assets/horn.png\";\nimport tick from \"../../assets/tick.png\";\nimport prs from \"../../assets/prs.png\";\n\nimport linkedin from \"../../assets/linkedin.svg\";\nimport insta from \"../../assets/insta.svg\";\nimport fb from \"../../assets/facebook.svg\";\n\nimport check from \"../../assets/check.svg\";\nimport purse from \"../../assets/purse.svg\";\nimport shield from \"../../assets/shield3.png\";\nimport kid from \"../../assets/kid.svg\";\nimport logoref from \"../../assets/logoref.png\";\nimport firebase from 'firebase/app';\nimport 'firebase/auth';\nimport 'firebase/database';\nimport app from \"../../firebase/base\";\n\nconst Landing = ({history}) => {\n    const user = firebase.auth().currentUser\n    if (user) {\n        app\n            .database()\n            .ref(\"users\")\n            .child(\"influencers\")\n            .child(user.uid)\n            .child(\"is_completed\")\n            .get()\n            .then((r